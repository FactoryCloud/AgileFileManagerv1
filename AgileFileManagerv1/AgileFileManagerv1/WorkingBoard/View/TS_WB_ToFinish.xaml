<Page x:Class="AgileFileManagerv1.WorkingBoard.View.TS_WB_ToFinish"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AgileFileManagerv1.WorkingBoard.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="TS_WB_ToFinish">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Button Margin="10">
            <TextBlock TextAlignment="Center">Atender<LineBreak/>Llamada</TextBlock>
        </Button>
        <Button Margin="10" Grid.Column="1">
            <TextBlock TextAlignment="Center">Crear<LineBreak/>Expediente</TextBlock>
        </Button>
        <Button IsEnabled="False" Margin="10" Grid.Row="1">
            <TextBlock TextAlignment="Center">Ver<LineBreak/>Expediente</TextBlock>
        </Button>
        <Button IsEnabled="False" Margin="10" Grid.Row="1" Grid.Column="1">
            <TextBlock TextAlignment="Center">Iniciar<LineBreak/>Expediente</TextBlock>
        </Button>
        <Button IsEnabled="False" Margin="10" Grid.Column="0" Grid.Row="2">
            <TextBlock TextAlignment="Center">Terminar<LineBreak/>Expediente</TextBlock>
        </Button>
        <Button IsEnabled="False" Margin="10" Grid.Column="1" Grid.Row="2">
            <TextBlock TextAlignment="Center">Liberar<LineBreak/>Expediente</TextBlock>
        </Button>
    </Grid>
</Page>
